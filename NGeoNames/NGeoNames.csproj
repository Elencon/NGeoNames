<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <Authors>RobIII</Authors>
    <Company>RobIII</Company>
    <PackageId>NGeoNames</PackageId>
    <NoPackageAnalysis>true</NoPackageAnalysis>
    <Product>NGeoNames</Product>
    <Copyright>(C) 2014 - 2018 Devcorner.nl</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://raw.githubusercontent.com/RobThree/NGeoNames/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/RobThree/NGeoNames</PackageProjectUrl>
    <PackageTags>reverse-geocoding geocoding geocode reverse-geocode geocoder reverse-geocoder maps</PackageTags>
    <PackageReleaseNotes>* Add support for AlternateNamesV2</PackageReleaseNotes>
    <Description>Provides classes and methods for downloading, reading/parsing and utilizing files from GeoNames.org dumps.</Description>
    <PackageIconUrl>https://raw.githubusercontent.com/RobThree/NGeoNames/master/icon.png</PackageIconUrl>
    <Version>1.5.1</Version>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;RELEASE;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net45|AnyCPU'">
    <DocumentationFile>bin\Release\net45\NGeoNames.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="KdTree" />
    <PackageReference Include="System.IO.Compression" />
  </ItemGroup>
</Project>